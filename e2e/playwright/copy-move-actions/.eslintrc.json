{
  "extends": "../../../.eslintrc.json",
  "ignorePatterns": [
    "!**/*"
  ],
  "overrides": [
    {
      "files": [
        "*.ts"
      ],
      "parserOptions": {
        "project": [
          "e2e/playwright/copy-move-actions/tsconfig.e2e.json"
        ],
        "createDefaultProgram": true
      },
      "plugins": [
        "rxjs",
        "unicorn"
      ],
      "rules": {
        "@typescript-eslint/no-floating-promises": "off"
      }
    }
  ]
}
